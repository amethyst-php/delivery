{
    "name": "amethyst/delivery",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-delivery", "delivery"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/time-range": "0.2.*",
        "amethyst/address": "0.2.*",
        "amethyst/shipment-zone": "0.2.*",
        "amethyst/price": "0.2.*",
        "amethyst/owner": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "amethyst/foo": "0.2.*",
        "amethyst/user": "0.2.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "delivery",
            "namespace": "Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\DeliveryServiceProvider"
            ]
        }
    }
}
